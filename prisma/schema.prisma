generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  userName  String
  telegamId BigInt   @unique // Int oâ€˜rniga BigInt ishlatamiz
  createdAt DateTime @default(now())
}

model Product {
  id    String @id @default(uuid())
  name  String
  price Int
  color String
}